    "version": "0.2.0",
    "configurations": [
        {
            "args": [
                "--backward_maxiter", "30", "backward_tol", "1e-01000"
            ],
            "name": "Gulp task",
            "program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
            "request": "launch",
            "runtimeExecutable": "ts-node",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        


        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
    ]
}